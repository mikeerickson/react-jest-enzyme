{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "no-undef": 0,
    "padded-blocks": 0,
    "semi": 0,
    "no-var": 0,
    "no-multi-spaces": 0,
    "spaced-comment": 0,
    "arrow-spacing": 0,
    "eol-last": 0,
    "comma-dangle": 0,
    "pace-before-function-paren": 0,
    "no-unused-vars": 0,
    "react/jsx-no-undef": 0,
    "space-before-function-paren": 0,
    "object-shorthand": 0,
    "func-names": 0,
    "react/prop-types": 0,
    "no-floating-decimal": 0,
    "react/prefer-statless-function": 0,
    "no-useless-construtor": 0,
    "react/jsx-space-before-closing": 0,
    "react/prefer-es6-class": 0,
    "key-spacing": 0,
    "object-curly-spacing": 0,
    "max-len": 0,
    "indent": 0,
    "prefer-arrow-callback": 0,
    "no-console": 0,
    "react/prefer-stateless-function": 0,
    "no-useless-constructor": 0
  }
}


