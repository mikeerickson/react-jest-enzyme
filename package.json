{
  "name": "react-gulp-enzyme",
  "version": "0.0.1",
  "description": "Sample React Jest Enzyme Gulp Webpack",
  "main": "index.js",
  "scripts": {
    "test": "gulp eslint test:mocha && jest",
    "test:jest": "gulp eslint && jest",
    "test:mocha": "mocha --compilers js:babel-core/register specs/.setup.js specs/**/*Spec.js --reporter mocha-unfunk-reporter",
    "test2": "mocha --compilers js:babel-core/register specs/.setup.js specs/**/*Spec.js --reporter mocha-unfunk-reporter"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/react-dom",
      "./node_modules/react-addons-test-utils"
    ],
    "testFileExtensions": [
      "es6",
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "es6",
      "js",
      "jsx"
    ],
    "collectCoverage": true
  },
  "author": "Mike Erickson <codedungeon@gmail.com> (https://github.com/mikeerickson)",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-jest": "^9.0.3",
    "babel-polyfill": "^6.7.2",
    "babel-preset-airbnb": "^1.1.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "bootstrap": "^3.3.6",
    "browserify": "^13.0.0",
    "cd-utils": "^0.3.1",
    "chai": "^3.5.0",
    "enzyme": "^2.1.0",
    "eslint": "^2.4.0",
    "eslint-plugin-react": "^4.2.3",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-messenger": "^0.13.1",
    "gulp-mocha": "^2.2.0",
    "istanbul": "^0.4.2",
    "jasmine-reporters": "^2.1.1",
    "jest-cli": "^0.9.2",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "mocha-unfunk-reporter": "^0.4.0",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "require-dir": "^0.3.0",
    "shelljs": "^0.6.0",
    "sinon": "^1.17.3",
    "vinyl-source-stream": "^1.1.0",
    "watchman": "^0.1.8"
  },
  "dependencies": {
    "react-datagrid": "^2.0.2"
  },
  "keywords": [
    "react",
    "jest",
    "enzyme",
    "gulp",
    "webpack"
  ]
}
